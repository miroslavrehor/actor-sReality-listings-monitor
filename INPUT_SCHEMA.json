{
  "title": "Input Schema for sReality Listings Monitor",
  "description": "Below you can provide object and operation types, as well as address to search for and monitor real estate ads. You could also apply filters and subtypes to narrow your search.",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "location": {
      "title": "Location",
      "type": "string",
      "description": "Desired street address/location",
      "editor": "textfield",
      "prefill": "Říčany u Prahy, Czechia",
      "example": "Říčany u Prahy, Czechia"
    },
    "offerType": {
      "title": "Operation Type",
      "type": "string",
      "description": "Operation Type: sale, rent or auction",
      "editor": "select",
      "enum": [
        "sale",
        "rent",
        "auction"
      ],
      "enumTitles": [
        "Sale",
        "Rent",
        "Auction"
      ],
      "example": "sale",
      "prefill": "sale"
    },
    "type": {
      "title": "Object Type",
      "type": "string",
      "description": "Object Type: apartment, house, land, commercial or other",
      "editor": "select",
      "enum": [
        "apartment",
        "house",
        "land",
        "commercial",
        "other"
      ],
      "enumTitles": [
        "Apartment",
        "House",
        "Land",
        "Commercial",
        "Other"
      ],
      "example": "apartment",
      "prefill": "apartment"
    },
    "maxPages": {
      "title": "Max number of pages",
      "type": "integer",
      "description": "Max number of search pages to process (default is 1)",
      "editor": "number",
      "nullable": true,
      "prefill": 1
    },
    "notificationsEmail": {
      "title": "Email for Notifications",
      "type": "string",
      "description": "",
      "editor": "textfield",
      "example": "email@example.com"
    },
    "proxyConfiguration": {
      "title": "Proxy Configuration",
      "type": "object",
      "description": "",
      "prefill": {
        "useApifyProxy": false
      },
      "editor": "proxy"
    },
    "priceMin":{
      "title": "From",
      "type": "integer",
      "description": "Minimum Price",
      "sectionCaption": "Price",
      "editor": "number",
      "unit": "$",
      "nullable": true
    },
    "priceMax":{
      "title": "To",
      "type": "integer",
      "description": "Maximum Price",
      "editor": "number",
      "unit": "$",
      "nullable": true
    },
    "areaMin":{
      "title": "From",
      "type": "integer",
      "description": "Minimum Area",
      "sectionCaption": "Usable Area",
      "editor": "number",
      "unit": "m2",
      "nullable": true
    },
    "areaMax":{
      "title": "To",
      "type": "integer",
      "description": "Maximum Area",
      "editor": "number",
      "unit": "m2",
      "nullable": true
    },
    "apartment:1kk": {
